{
  "name": "hunter-galloway-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "astrobook": "astro dev --config ./astro.config.mjs",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "check:tokens:hex": "grep -R --line-number -E '#[0-9a-fA-F]{3,8}\\b' src/components || true",
    "check:tokens:func": "grep -R --line-number -E 'rgba?\\(|hsla?\\(' src/components || true",
    "check:tokens": "npm run -s check:tokens:hex && npm run -s check:tokens:func",
    "prepr": "npm run -s build && npm run -s check:tokens",
    "check": "node scripts/check-no-literals.mjs && node scripts/check-atomic-boundaries.mjs && node scripts/check-registry.mjs && node scripts/check-similarity.mjs && node scripts/check-demo-route.mjs",
    "check:demo": "node scripts/check-demo-route.mjs",
    "new:component": "node scripts/generate-component.mjs",
    "snap": "node scripts/capture-snapshots.mjs"
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@nanostores/react": "^1.0.0",
    "@storyblok/astro": "^7.2.3",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "astro": "^5.12.8",
    "autonumeric": "^4.10.8",
    "axios": "^1.11.0",
    "nanostores": "^1.0.1",
    "rc-slider": "^11.1.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "swiper": "^11.2.10",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "fast-glob": "^3.3.2",
    "globby": "^14.0.2",
    "puppeteer": "^22.15.0",
    "@chromatic-com/storybook": "^4.1.0",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/addon-vitest": "^9.1.1",
    "@storybook/html-vite": "^9.1.1",
    "@storybook/react-vite": "^9.1.1",
    "@storybook/web-components-vite": "^9.1.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "astrobook": "^0.8.9",
    "chromatic": "^11.29.0",
    "lit": "^3.3.1",
    "playwright": "^1.54.2",
    "storybook": "^9.1.1",
    "vitest": "^3.2.4"
  }
}
